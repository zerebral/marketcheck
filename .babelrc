{
  "env": {
    "development": {
      "presets": "next/babel"
    },
    "production": {
      "presets": "next/babel"
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ]
    }
  },
  "plugins": [
    ["root-import", [{
      "rootPathPrefix": "~",
      "rootPathSuffix": "components"
      }, {
      "rootPathPrefix": "@",
      "rootPathSuffix": "containers"
      }, {
      "rootPathPrefix": "%",
      "rootPathSuffix": "lib"
      }
    ]],
    "inline-react-svg",
    ["styled-components", {
      "ssr": true,
      "displayName": true,
      "preprocess": false
    }]
  ]
}
