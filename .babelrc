{
  "env": {
    "development": {
      "presets": "next/babel"
    },
    "production": {
      "presets": "next/babel"
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ]
    }
  },
  "plugins": [
    ["root-import", {
      "rootPathSuffix": "components"
    }],
    ["styled-components", {
      "ssr": true,
      "displayName": true,
      "preprocess": false
      }],
    "inline-react-svg"
  ]
}
